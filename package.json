    "@types/aws-lambda": "^8.10.33",
    "@types/node": "^12.11.1",
    "@types/sharp": "^0.23.0",
    "@types/uuid": "^3.4.5",
    "aws-cdk": "^1.14.0",
    "concurrently": "^5.0.0",
    "serverless": "^1.54.0",
    "serverless-dynamodb-local": "^0.2.38",
    "serverless-offline": "^5.12.0",
    "serverless-s3-local": "^0.5.2",
    "trash-cli": "^3.0.0",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.1.0",
    "typescript": "~3.5.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "@angular/animations": "^8.2.11",
