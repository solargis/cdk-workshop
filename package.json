{
  "name": "cdk-workshop",
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently -c gray,yellow,green npm:api:watch npm:api:serve npm:web:serve --kill-others-on-fail",
    "build": "npm run api:build && npm run web:build",
    "api:build": "trash dist/api && webpack --config lib/webpack.api.js",
    "api:watch": "npm run api:build -- --watch --progress",
    "api:serve": "sls dynamodb install && sls offline start",
    "web:build": "trash dist/web && ng build --aot --prod",
    "web:serve": "ng serve"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.12",
